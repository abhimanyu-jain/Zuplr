<div class="container top-space stylist">
	<div class="row top-menu">
		<div class="col-sm-2 active">My Sessions</div>
		<!-- <div class="col-sm-2">Users</div> -->
	</div>
	<table class="table table-striped">
        <thead>
            <tr>
                <th>Session Id</th>
                <th>Name</th>
                <th>Phone number</th>
                <th>Data</th>
                <th>Profile</th>
                <th>Status</th>
            </tr>
        </thead>
        <tbody>
             <% @deliveries.each do |delivery| %>
		        <tr>
		          	<td><%= delivery.session_number %></td>
		          	<td><%= delivery.user.email %></td>
		          	<td><%= delivery.user.userprofile.phonenumber %></td>
		          	<td></td>
		            <td>
		            	<% if delivery.user.identities.where('provider = ?', 'facebook').present? %>
		              	<a target="_blank" href="http://facebook.com/<%= delivery.user.identities.where('provider = ?', 'facebook')[0].uid %>"><%= delivery.user.identities.where('provider = ?', 'facebook')[0].name %></a>
		            	<% end %>
		          	</td>
          			<td><%= delivery.status %></td>
		      <% end %>
        </tbody>
    </table>
</div>