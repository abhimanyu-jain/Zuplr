<div style ="margin-left: 50px;" media="screen">
	<% @user.each do |u| %>
		<h4><strong>Registered On : </strong><%= u.created_at.to_date.to_s + " " +  u.created_at.to_s(:time) %></h4>
		<br />
		<h4><strong>Email : </strong><%= u.email %></h4>
		<br />
		<h4><strong>City : </strong><%= u.city %></h4>
		<br />
		<h4><strong>Phone : </strong><%= u.phonenumber %></h4>
		<br />
		<h4><strong>Style data : </strong><%= u.data %></h4>
	<% end %>
</div>

