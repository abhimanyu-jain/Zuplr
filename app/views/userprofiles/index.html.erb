<div class="container top-space">

  <p id="notice"><%= notice %></p>

  <h1>Listing Userprofiles</h1>

  <table class="table table-striped">
    <thead>
      <tr>
        <th>S.No</th>
        <th>Data</th>
        <th>City</th>
        <th>Phonenumber</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @userprofiles.each do |userprofile| %>
        <tr>
          <td></td>
          <td><%= userprofile.data %></td>
          <td><%= userprofile.city %></td>
          <td><%= userprofile.phonenumber %></td>
          <td><%= link_to 'Show', userprofile %></td>
          <td><%= link_to 'Edit', edit_userprofile_path(userprofile) %></td>
          <td><%= link_to 'Destroy', userprofile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to '+ New UserProfile', new_userprofile_path, :class => 'btn btn-success pull-right', :style => 'color:white;margin:0px 0px 0px 20px;' %>
  <%= link_to 'Back', '/admin/dashboard', :class => 'btn btn-success pull-right', :style => 'color:white' %>

</div>